<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Selenium_Framework_Automation_Suite" verbose="1">
    <listeners>
        <listener class-name="com.rbhatt.selenium.TestComponents.AnnotationTransformer"/>
    </listeners>
    <test thread-count="5" name="CucumberRunner">
        <classes>
            <class name="cucumber.TestNGTestRunner"/>
        </classes>
    </test>
</suite>